pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo "Compile the Code.........."
                sh "mvn Compile"
            }
        }
        stage('Test') {
            steps {
                echo "Testing the Code.........."
                sh "mvn test"
            }
        }
        stage('Package') {
            steps {
                echo "Packiging the Code.........."
                sh "mvn package"
            }
        }
        stage('Install') {
            steps {
                echo "installing the Code.........."
                sh "mvn install"
            }
        }
    }
}
