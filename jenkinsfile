pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo "Compile the Code.........."
                bash "mvn Compile"
            }
        }
        stage('Test') {
            steps {
                echo "Testing the Code.........."
                bash "mvn test"
            }
        }
        stage('Package') {
            steps {
                echo "Packiging the Code.........."
                bash "mvn -Dmaven.test.failure.ignore=true package"
            }
        }
        stage('Install') {
            steps {
                echo "installing the Code.........."
                bash "mvn -Dmaven.test.failure.ignore=true install"
            }
        }
    }
}
